{
  "name": "@geoman-io/maplibre-geoman-free",
  "private": false,
  "version": "0.4.3",
  "type": "module",
  "description": "A MapLibre Plugin For Drawing and Editing Geometry Layers",
  "homepage": "https://geoman.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/geoman-io/maplibre-geoman.git"
  },
  "author": {
    "name": "Geoman.io",
    "email": "sales@geoman.io",
    "url": "http://geoman.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geoman-io/maplibre-geoman/issues"
  },
  "module": "dist/maplibre-geoman.es.js",
  "main": "dist/maplibre-geoman.umd.js",
  "types": "dist/maplibre-geoman.d.ts",
  "scripts": {
    "dev": "vite -- --base-map=maplibre",
    "dev:mapbox": "vite --port 3200 -- --base-map=mapbox",
    "build": "vite build -- --base-map=maplibre && npm run build:types",
    "build:mapbox": "vite build -- --base-map=mapbox && npm run build:types:mapbox",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias && rollup -c rollup.config.dts.js && rm -rf dist/types",
    "build:types:mapbox": "tsc --emitDeclarationOnly && tsc-alias && rollup -c rollup.config.dts.js --base-map=mapbox && rm -rf dist/types",
    "ts": "tsc --noEmit",
    "ts-watch": "tsc --noEmit --watch",
    "ts:autofix": "ts-autofix",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.svelte --fix",
    "check": "npm run lint && npm run ts",
    "testserver": "vite --port 4000 --mode test -- --gm-version=pro",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug"
  },
  "dependencies": {
    "@turf/area": "^7.2.0",
    "@turf/bbox": "^7.2.0",
    "@turf/bearing": "^7.2.0",
    "@turf/boolean-intersects": "^7.2.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/boolean-point-on-line": "^7.2.0",
    "@turf/boolean-valid": "^7.2.0",
    "@turf/buffer": "^7.2.0",
    "@turf/centroid": "^7.2.0",
    "@turf/circle": "^7.2.0",
    "@turf/clone": "^7.2.0",
    "@turf/combine": "^7.2.0",
    "@turf/difference": "^7.2.0",
    "@turf/distance": "^7.2.0",
    "@turf/flatten": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/length": "^7.2.0",
    "@turf/line-intersect": "^7.2.0",
    "@turf/line-split": "^7.2.0",
    "@turf/line-to-polygon": "^7.2.0",
    "@turf/meta": "^7.2.0",
    "@turf/polygon-to-line": "^7.2.0",
    "@turf/rewind": "^7.2.0",
    "@turf/transform-rotate": "^7.2.0",
    "@turf/transform-scale": "^7.2.0",
    "@turf/union": "^7.2.0",
    "@turf/unkink-polygon": "^7.2.0",
    "dompurify": "^3.2.4",
    "geojson-path-finder": "^2.0.2",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.9.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.52.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-svelte": "^3.0.2",
    "globals": "^16.0.0",
    "maplibre-gl": "^5.2.0",
    "playwright": "^1.52.0",
    "rollup-plugin-dts": "^6.1.1",
    "sass": "^1.85.1",
    "svelte": "^5.21.0",
    "svelte-eslint-parser": "^1.0.0",
    "svelte-preprocess": "^6.0.3",
    "ts-autofix": "^1.0.0",
    "tsc-alias": "^1.8.11",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2",
    "vite": "^6.3.5",
    "vite-svg-loader": "^5.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
