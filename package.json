{
  "name": "@geoman-io/maplibre-geoman-free",
  "private": false,
  "version": "0.4.8",
  "type": "module",
  "description": "A MapLibre Plugin For Drawing and Editing Geometry Layers",
  "homepage": "https://geoman.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/geoman-io/maplibre-geoman.git"
  },
  "author": {
    "name": "Geoman.io",
    "email": "sales@geoman.io",
    "url": "https://geoman.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geoman-io/maplibre-geoman/issues"
  },
  "module": "dist/maplibre-geoman.es.js",
  "main": "dist/maplibre-geoman.umd.js",
  "types": "dist/maplibre-geoman.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias && rollup -c rollup.config.dts.js && rm -rf dist/types",
    "ts": "tsc --noEmit",
    "ts-watch": "tsc --noEmit --watch",
    "ts:autofix": "ts-autofix",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.svelte --fix",
    "formatter": "prettier src tests --check",
    "formatter:fix": "prettier src tests --write",
    "check": "npm run formatter && npm run lint && npm run ts",
    "testserver": "vite --port 4000 --mode test",
    "test": "VITE_GEOMAN_VERSION=free playwright test",
    "test:headed": "VITE_GEOMAN_VERSION=free playwright test --headed",
    "test:ui": "VITE_GEOMAN_VERSION=free playwright test --ui",
    "test:debug": "VITE_GEOMAN_VERSION=free playwright test --debug"
  },
  "dependencies": {
    "@turf/area": "^7.2.0",
    "@turf/bbox": "^7.2.0",
    "@turf/bearing": "^7.2.0",
    "@turf/boolean-intersects": "^7.2.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/boolean-point-on-line": "^7.2.0",
    "@turf/boolean-valid": "^7.2.0",
    "@turf/buffer": "^7.2.0",
    "@turf/centroid": "^7.2.0",
    "@turf/circle": "^7.2.0",
    "@turf/clone": "^7.2.0",
    "@turf/combine": "^7.2.0",
    "@turf/difference": "^7.2.0",
    "@turf/distance": "^7.2.0",
    "@turf/flatten": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/length": "^7.2.0",
    "@turf/line-intersect": "^7.2.0",
    "@turf/line-split": "^7.2.0",
    "@turf/line-to-polygon": "^7.2.0",
    "@turf/meta": "^7.2.0",
    "@turf/polygon-to-line": "^7.2.0",
    "@turf/rewind": "^7.2.0",
    "@turf/transform-rotate": "^7.2.0",
    "@turf/transform-scale": "^7.2.0",
    "@turf/union": "^7.2.0",
    "@turf/unkink-polygon": "^7.2.0",
    "dompurify": "^3.2.6",
    "geojson-path-finder": "^2.0.2",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.9.2",
    "type-fest": "^4.41.0"
  },
  "peerDependencies": {
    "maplibre-gl": "^5.7.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.18.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "rollup-plugin-dts": "^6.2.3",
    "sass": "^1.91.0",
    "svelte": "^5.38.3",
    "svelte-eslint-parser": "^1.3.1",
    "svelte-preprocess": "^6.0.3",
    "ts-autofix": "^1.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-svg-loader": "^5.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
