{
  "name": "@geoman-io/mapbox-geoman-free",
  "version": "0.7.0",
  "private": false,
  "description": "A Mapbox Plugin For Drawing and Editing Geometry Layers",
  "homepage": "https://geoman.io",
  "bugs": {
    "url": "https://github.com/geoman-io/maplibre-geoman/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Geoman.io",
    "email": "sales@geoman.io",
    "url": "https://geoman.io"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/geoman-io/maplibre-geoman.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/mapbox-geoman.umd.js",
  "module": "dist/mapbox-geoman.es.js",
  "types": "dist/mapbox-geoman.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mapbox-geoman.d.ts",
      "import": "./dist/mapbox-geoman.es.js",
      "require": "./dist/mapbox-geoman.umd.js"
    },
    "./dist/mapbox-geoman.css": "./dist/mapbox-geoman.css"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "vite ../.. --config vite.config.ts",
    "build": "rimraf dist && vite build --config vite.config.ts && npm run build:types && node ../../scripts/verify-variant-bundle.mjs mapbox dist",
    "testserver": "vite ../.. --config vite.config.ts --host 127.0.0.1 --port 4001 --mode test",
    "build:types": "tsc -p tsconfig.types.json --emitDeclarationOnly && tsc-alias -p tsconfig.types.json && rollup -c rollup.config.dts.js && node ../../scripts/verify-variant-types.mjs mapbox dist/mapbox-geoman.d.ts && rimraf dist/types",
    "ts": "tsc --noEmit -p ../../tsconfig.mapbox.json",
    "lint": "oxlint src",
    "pack": "npm run build && npm pack"
  },
  "dependencies": {
    "@turf/area": "^7.3.4",
    "@turf/bbox": "^7.3.4",
    "@turf/bearing": "^7.3.4",
    "@turf/boolean-intersects": "^7.3.4",
    "@turf/boolean-point-in-polygon": "^7.3.4",
    "@turf/boolean-point-on-line": "^7.3.4",
    "@turf/boolean-valid": "^7.3.4",
    "@turf/buffer": "^7.3.4",
    "@turf/centroid": "^7.3.4",
    "@turf/circle": "^7.3.4",
    "@turf/clone": "^7.3.4",
    "@turf/combine": "^7.3.4",
    "@turf/destination": "^7.3.4",
    "@turf/difference": "^7.3.4",
    "@turf/distance": "^7.3.4",
    "@turf/flatten": "^7.3.4",
    "@turf/helpers": "^7.3.4",
    "@turf/invariant": "^7.3.4",
    "@turf/length": "^7.3.4",
    "@turf/line-intersect": "^7.3.4",
    "@turf/line-split": "^7.3.4",
    "@turf/line-to-polygon": "^7.3.4",
    "@turf/meta": "^7.3.4",
    "@turf/polygon-to-line": "^7.3.4",
    "@turf/rewind": "^7.3.4",
    "@turf/transform-rotate": "^7.3.4",
    "@turf/transform-scale": "^7.3.4",
    "@turf/union": "^7.3.4",
    "@turf/unkink-polygon": "^7.3.4",
    "@types/geojson": "^7946.0.16",
    "dompurify": "^3.3.1",
    "lodash-es": "^4.17.23",
    "loglevel": "^1.9.2",
    "type-fest": "^5.4.4"
  },
  "peerDependencies": {
    "mapbox-gl": ">=3.18.0"
  }
}
